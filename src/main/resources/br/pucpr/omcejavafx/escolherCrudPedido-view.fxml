<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="br.pucpr.omcejavafx.PaginaEscolherCrudPedido"
      prefHeight="400.0" prefWidth="600.0">
    <padding>
        <Insets top="20.0" right="20.0" bottom="20.0" left="20.0"/>
    </padding>
    <Label fx:id="inserir"/>
    <Label fx:id="teste"/>
    <Button text="inserir!" onAction="#onInserir"/>
    <padding>
        <Insets top="20.0" right="40.0" bottom="20.0" left="20.0"/>
    </padding>
    <Label fx:id="consultar"/>
    <Button text="consultar!" onAction="#onConsultar"/>
    <padding>
        <Insets top="20.0" right="20.0" bottom="20.0" left="20.0"/>
    </padding>
    <Label fx:id="atualizar"/>
    <Button text="atualizar!" onAction="#onAtualizar"/>
    <padding>
        <Insets top="20.0" right="20.0" bottom="20.0" left="20.0"/>
    </padding>
    <Label fx:id="excluir"/>
    <Button text="excluir!" onAction="#onExcluir"/>
</VBox>
