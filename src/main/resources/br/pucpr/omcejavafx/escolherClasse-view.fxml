<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Label?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="br.pucpr.omcejavafx.PaginaEscolherClasse"
      prefHeight="400.0" prefWidth="600.0">

    <padding>
        <Insets top="20.0" right="20.0" bottom="20.0" left="20.0"/>
    </padding>
    <Label fx:id="Pagamento"/>
    <Button text="Pagamento!" onAction="#onPagamento"/>
    <padding>
        <Insets top="20.0" right="40.0" bottom="20.0" left="20.0"/>
    </padding>
    <Label fx:id="Pedido"/>
    <Button text="Pedido!" onAction="#onPedido"/>
    <padding>
        <Insets top="20.0" right="20.0" bottom="20.0" left="20.0"/>
    </padding>
    <Label fx:id="Usuario"/>
    <Button text="Usuario!" onAction="#onUsuario"/>
    <padding>
        <Insets top="20.0" right="20.0" bottom="20.0" left="20.0"/>
    </padding>
    <Label fx:id="Produto"/>
    <Button text="Produto!" onAction="#onProduto"/>

</VBox>
