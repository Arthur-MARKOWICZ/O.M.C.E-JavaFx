<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="br.pucpr.omcejavafx.PaginaListarAvaliacaoProduto"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <VBox spacing="10.0" layoutX="100.0" layoutY="50.0">
            <padding>
                <Insets top="20.0" right="20.0" bottom="20.0" left="20.0"/>
            </padding>
            <children>
                <Label text="Lista de Avaliações:" />
                <TextArea fx:id="txtListaAvaliacoes" editable="false" prefRowCount="20" />
            </children>
        </VBox>
    </children>
</AnchorPane>
